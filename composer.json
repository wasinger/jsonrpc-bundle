{
    "name":"wa72/jsonrpc-bundle",
    "description":"JSON-RPC server for Symfony: exposes services registered in the service container as JSON-RPC webservices",
    "keywords":["json", "rpc", "jsonrpc", "json-rpc", "webservice", "symfony", "symfony-bundle" ],
    "homepage":"https://github.com/wasinger/jsonrpc-bundle",
    "license":"MIT",
    "type": "symfony-bundle",
    "authors":[
        {
            "name":"Christoph Singer",
            "email":"dev@ccss.de"
        }
    ],
    "require":{
        "php":">=8.0",
        "ext-json": "*",
        "symfony/framework-bundle":"^5.4|^6|^7",
        "symfony/yaml": "^5.4|^6|^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "doctrine/annotations": "^1.11",
        "jms/serializer-bundle": "^3|^4|^5",
        "symfony/property-access": "^5.4|^6.0|^7.0",
        "symfony/property-info": "^5.4|^6.0|^7.0",
        "symfony/serializer": "^5.4|^6.0|^7.0"
    },
    "suggest": {
        "jms/serializer": "If you want to use JMS Serializer for serialization/deserialization",
        "symfony/serializer": "For using Symfony's Serializer component"
    },
    "autoload":{
        "psr-4":{
            "Wa72\\JsonRpcBundle\\":""
        }
    }
}
